{
  "name": "screwmycode-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn dev",
    "build": "tsc --build tsconfig.json",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "dev": "yarn build && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/bin/www.js",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "fast-xml-parser": "^3.17.4",
    "http-errors": "~1.8.0",
    "mongoose": "^5.9.22",
    "morgan": "~1.10.0",
    "node-fetch": "^2.6.0",
    "snyk": "^1.360.0",
    "ytdl-core": "^2.1.7"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^1.2.11",
    "@bamdadsabbagh/stylelint-config": "^1.2.10",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.6.3",
    "@types/mongoose": "^5.7.30",
    "@types/morgan": "^1.9.1",
    "@types/node-fetch": "^2.5.7",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  },
  "snyk": true
}
