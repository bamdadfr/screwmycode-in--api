{
  "name": "screwmycode-api",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "yarn dev",
    "build": "tsc --build tsconfig.json",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "dev": "yarn clean && yarn build && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/app.bin.js",
    "test": "jest",
    "test:detect": "jest --detectOpenHandles"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "fast-xml-parser": "^3.17.4",
    "http-errors": "~1.8.0",
    "mongoose": "^5.9.22",
    "morgan": "~1.10.0",
    "node-fetch": "^2.6.0",
    "ytdl-core": "^3.1.2"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^1.3.1",
    "@bamdadsabbagh/stylelint-config": "^1.2.12",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^26.0.4",
    "@types/mongoose": "^5.7.30",
    "@types/morgan": "^1.9.1",
    "@types/node-fetch": "^2.5.7",
    "concurrently": "^5.2.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1"
  }
}
