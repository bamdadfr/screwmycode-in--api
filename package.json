{
  "name": "screwmycode-api",
  "version": "1.2.376",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "clean": "rm -rf dist",
    "dev:start": "yarn mongo:start && yarn clean && yarn build && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch:ts\" \"yarn watch:node\"",
    "dev:stop": "yarn mongo:stop && yarn clean",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "mongo:start": "docker-compose -f dev/docker-compose.yml up -d",
    "mongo:stop": "docker-compose -f dev/docker-compose.yml down",
    "package:clean": "rm -rf node_modules",
    "package:reinstall": "yarn package:clean && yarn",
    "package:sort": "npx sort-package-json",
    "start": "yarn dev:start",
    "test": "jest",
    "test:detect": "jest --detectOpenHandles",
    "watch:node": "nodemon dist/app.bin.js",
    "watch:ts": "tsc -w"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.2.0",
    "express": "~4.17.1",
    "fast-xml-parser": "^3.17.4",
    "http-errors": "~1.8.0",
    "mongoose": "^5.9.29",
    "morgan": "~1.10.0",
    "node-fetch": "^2.6.0",
    "ytdl-core": "^4.0.3"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^1.3.29",
    "@bamdadsabbagh/stylelint-config": "^1.2.43",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.9",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node-fetch": "^2.5.7",
    "concurrently": "^5.3.0",
    "jest": "^26.4.0",
    "nodemon": "^2.0.4",
    "supertest": "^6.0.1",
    "ts-jest": "^26.2.0"
  }
}
