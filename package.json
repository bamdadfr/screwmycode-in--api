{
  "name": "screwmycode-api",
  "version": "1.2.500",
  "license": "MIT",
  "scripts": {
    "dev": "yarn dev:stop && yarn dev:start",
    "dev:start": "yarn dev:start:mongo && yarn dev:start:js",
    "dev:start:js": "pm2-runtime start ecosystem.config.js --only dev",
    "dev:start:mongo": "docker-compose -f dev/docker-compose.yml up -d",
    "dev:stop": "yarn dev:stop:mongo",
    "dev:stop:mongo": "docker-compose -f dev/docker-compose.yml down",
    "lint": "eslint --fix --ext .js src",
    "package:clean": "rm -rf node_modules",
    "package:reinstall": "yarn package:clean && yarn",
    "package:sort": "npx sort-package-json",
    "start": "pm2-runtime start ecosystem.config.js --only production",
    "stop": "yarn dev:stop",
    "test": "jest",
    "test:detect": "jest --detectOpenHandles"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.3.1",
    "express": "~4.17.1",
    "fast-xml-parser": "^3.17.4",
    "http-errors": "~1.8.0",
    "mongoose": "^5.9.29",
    "morgan": "~1.10.0",
    "node-fetch": "^2.6.0",
    "pm2": "^4.5.4",
    "ytdl-core": "^4.0.3"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^1.5.0",
    "jest": "^26.4.0",
    "supertest": "^6.0.1"
  }
}
